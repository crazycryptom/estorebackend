datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL)
}

generator client {
    provider = "cargo prisma"
    output = "../tests/db"

    module_path = "crate::db"
    clientFormat = "directory"
}

model SomeModel {
    id  String @id
    some_column Json?
    some_enum SomeEnum
    some_enum_optional SomeEnum?
    enum_list SomeEnum[]
}

model AnotherModel {
    id String @id
    string_array String[]
    another_array String[]

    json Json
}

enum SomeEnum {
    A
    B
}