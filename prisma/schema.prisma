datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
}

generator client {
    provider = "cargo prisma"
    output = "../src/prisma.rs"
}

model SomeModel {
    id  String @id
    some_column Json?
    some_enum SomeEnum
    some_enum_optional SomeEnum?
    enum_list SomeEnum[]
}

model AnotherModel {
    id String @id
    string_array String[]
    another_array String[]

    json Json
}

enum SomeEnum {
    A
    B
}